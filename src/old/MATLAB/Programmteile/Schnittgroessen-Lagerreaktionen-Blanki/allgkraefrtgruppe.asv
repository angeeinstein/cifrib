function [] = Akg()

global main;

[~,B1] = main.Bearing;
Lag = zeros(0,B1);

% summe aller kr√§fte 
SF = 0;
% Implement all external forces
[~,v] = size(main.Force);
for i=1:v
    SF = SF + main.Force(1,i);
end

% Bearings
for i=1:B1
    SF = SF + Lag(1,B1);
end

% sum of moments 
SM = 0;
% Forces
[~,v] = size(main.Force);
for i=1:v
    SM = SM + main.Force(1,i)* main.Force(2,i);
end

% Bearings
for i=1:B1
    SM = SM + Lag(1,B1) * main.Bearing(1,B1);
end




